<div class="row">

  <div class="col-md-6">

    <div class="venueInfoSection">
      <div class="flex-container">
        <div class="flex-item">
          <div class="venueInfo">
            <img src="https://lh3.googleusercontent.com/-LvroGvBAXVA/WWPkQxrzvCI/AAAAAAABhv0/vpiQYmET1c8G9Loh9MFMid19kPUEFHF4gCLIBGAYYCw/w180-h180-n-k-no/" class="img-rounded" alt="Pete's Cafe and Burrito Haven">
            <span class="label venueScore">{{model.allVenues.rating}}</span>
            <span class="label venueCost">{{model.allVenues.price.currency}}</span>
            <div class="restaurantDetails">
              <h3 class="restaurantName">{{model.allVenues.name}}</h3>
              <p class="restaurantDescription">{{model.allVenues.location.address}},{{model.allVenues.location.country}}</p>
              <p>{{model.url}}</p>
              <span class="label venueContact">{{result.venue.contact.phone}}</span>
            </div>
          </div>
          <div class="venueActionSection">
            <button class="tipsbtn" type="button" name="button">Tips and Reviews</button>
            <div class="venueRateSection">
              <span class="venueUpvote"><i class="fa fa-heart-o fa-2x fa-fw" aria-hidden="true"></i>Upvote</span>
              <span class="venueDownvote"><i class="fa fa-thumbs-down fa-2x fa-fw" aria-hidden="true"></i>Downvote</span>
            </div>
          </div>
          <div class="venueReviews">
              <ul class="nav nav-tabs">
                <li class="active"><a href="#">All Reviews</a></li>
              </ul>
            </div>
          <div class="reviewSection">
            {{#each model.allReviews as |review|}}
              {{review-details review=review}}
            {{/each}}
        </div>
      </div>
    </div>
    </div>

  </div>

  <div class="col-md-6" id="venue-map">

    <!-- <img
    src="https://maps.googleapis.com/maps/api/staticmap?center='+{{model.allVenues.location.address}}+'&zoom=13&size=640x700&maptype=roadmap&markers=color:blue%7Clabel:S%7C'+{{model.allVenues.location.lat}}+','+{{model.allVenues.location.lng}}+'&key=AIzaSyAPbT83XDa5Inel4sYxFhGENS4FyUMuZSk"/>  -->
    <img src="https://maps.googleapis.com/maps/api/staticmap?center='+{{model.allVenues.location.address}}+'&zoom=13&size=640x640&sensor=false&maptype=roadmap&markers=color:red|{{model.allVenues.location.lat}},{{model.allVenues.location.lng}}&key=AIzaSyAPbT83XDa5Inel4sYxFhGENS4FyUMuZSk"/>
  </div>

  </div>
