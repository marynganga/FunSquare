<div class="venueInfoSection">
  <div class="flex-container">
    <div class="flex-item">
      <div class="venueInfo">
        <img src="https://lh3.googleusercontent.com/-LvroGvBAXVA/WWPkQxrzvCI/AAAAAAABhv0/vpiQYmET1c8G9Loh9MFMid19kPUEFHF4gCLIBGAYYCw/w180-h180-n-k-no/" class="img-rounded" alt="Pete's Cafe and Burrito Haven">
        <span class="label venueScore">Venue Mean Rating</span>
        <span class="label venueCost">Cost Estimation</span>
        <div class="restaurantDetails">
          <h4 class="restaurantName">Name of Restaurant</h4>
          <h4 class="restaurantDescription">Restaurant Description in terms of location</h4>
          <button class="actionSave" type="button" name="button"><i class="fa fa-bookmark-o fa-2x fa-fw"  aria-hidden="true"></i>Saved</button>
        </div>
      </div>
      <div class="venueActionSection">
        <button class="tipsbtn" type="button" name="button">Tips and Reviews</button>
        <div class="venueRateSection">
      <span class="likeVenue"><i class="fa fa-thumbs-up fa-2x fa-fw" aria-hidden="true"></i></span>
      <span class="dislikeVenue"><i class="fa fa-thumbs-down fa-2x fa-fw" aria-hidden="true"></i></span>
        </div>
      </div>
    </div>

  </div>
</div>
